int main(){
    //Definicion de variables
    int a;//David
    int b;//Antonio
    int aux;//Jorge
    a=2;
    b=3;
    printf("\nANTES DEL INTERCAMBIO: ");
    printf("\na = %d",a);
    printf("\nb = %d",b);
    //Ahora hacemos el intercambio
    aux=a;
    a=b;
    b=aux;    
    printf("\nDESPUES DEL INTERCAMBIO: ");
    printf("\na = %d",a);
